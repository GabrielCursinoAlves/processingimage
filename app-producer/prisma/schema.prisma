generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ProcessedImage {
  id           String   @id
  image_id     String
  file_path    String
  mime_type    String
  status       String   @default("pending")
  error_reason String?
  createdAt    DateTime @default(now())
}
